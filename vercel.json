{
  "version": 2,
  "installCommand": "cd apps/api && npm ci",
  "buildCommand": "cd apps/api && npm run vercel-build",
  "builds": [
    { "src": "apps/api/api/index.ts", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "apps/api/api/index.ts" }
  ],
  "functions": {
    "apps/api/api/index.ts": {
      "runtime": "nodejs18.x"
    }
  }
}


