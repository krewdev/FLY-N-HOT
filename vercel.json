{
  "version": 2,
  "name": "fly-in-high",
  "installCommand": "cd apps/api && npm ci && cd ../web && npm ci",
  "buildCommand": "cd apps/api && npm run vercel-build && cd ../web && npm run build",
  "outputDirectory": "apps/web/.next",
  "builds": [
    {
      "src": "apps/api/api/index.ts",
      "use": "@vercel/node",
      "config": {
        "runtime": "nodejs18.x"
      }
    },
    {
      "src": "apps/web/package.json",
      "use": "@vercel/next"
    }
  ],
  "routes": [
    {
      "src": "/api/(.*)",
      "dest": "apps/api/api/index.ts"
    },
    {
      "src": "/(.*)",
      "dest": "apps/web/$1"
    }
  ],
  "functions": {
    "apps/api/api/index.ts": {
      "runtime": "nodejs18.x"
    }
  }
}


